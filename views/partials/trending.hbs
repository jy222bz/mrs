<main id="main" class="sec2">
  <br> <br>
  {{#if rows.length}}
  <h2 class="headline">{{message1}} </h2>
  <br> <br>
  <h1 class="headline">{{message2}} </h1>
  <table class="table table-dark table-striped">
    <thead>
      <tr>
        <th scope="col">Title:</th>
        <th scope="col">Type:</th>
        <th scope="col">IMDB RATE:</th>
        <th scope="col">Check Picture</th>
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
      <tr>
        <td> {{this.id}}</td>
        <td> {{this.type}}</td>
        <td>{{this.rate}}</td>
        <td> <a href="/find/{{this.id}}"><i class="bi bi-shield-exclamation"></i< /a>
        </td>
        {{/each}}
    </tbody>
  </table>
  {{else}}
  <h1 class="headline">No Result.</h1>
  {{/if}}
</main>