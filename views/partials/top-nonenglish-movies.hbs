<main id="main" class="sec2">
  <br> <br>
  <h1 class="headline">Top Rated: Non-English Movie[s]: </h1>
  <table class="table table-dark table-striped">
    <thead>
      <tr>
        <th scope="col">Movie Name:</th>
        <th scope="col">Director:</th>
        <th scope="col">Category:</th>
        <th scope="col">Origin:</th>
        <th scope="col">Language:</th>
        <th scope="col">Rate:</th>
      </tr>
    </thead>
    <tbody>
      {{#if rows.length}}
      {{#each rows}}
      <tr>
        <td> {{this.name}}</td>
        <td> {{this.director}}</td>
        <td>{{this.category}}</td>
        <td> {{this.origin}}</td>
        <td>{{this.language}}</td>
        <td>{{this.rate}}</td>
        {{/each}}
    </tbody>
  </table>
  <br> <br>
  <h1 class="headline">The Facts are based on the information that is provided by the Reviewers!</h1>
  {{else}}
  <h1 class="headline">There is no data at this time.</h1>
  {{/if}}
</main>